    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: iRacingSafetycarGenerator",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": ["-dev"],
            "cwd": "${workspaceFolder}/src"
        },
        {
            "name": "Python Debugger: iRacingSafetycarGenerator (MacOS)",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": ["-dev", "-dwi"],
            "cwd": "${workspaceFolder}/src"
        }
    ]
}